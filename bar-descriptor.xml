<?xml version='1.0' encoding='UTF-8' standalone='no'?>
<qnx xmlns="http://www.qnx.com/schemas/application/1.0">
    <id>com.ruedigergad.meepasswords</id>
    <name>Passwords</name>
    <versionNumber>2.1.1</versionNumber>
    <description>(Mee-)Passwords is a simple tool to securely store short snippets of sensible or confidential data such as passwords and the like.</description>
    <initialWindow>
        <aspectRatio>portrait</aspectRatio>
        <autoOrients>false</autoOrients>
        <systemChrome>none</systemChrome>
        <transparent>false</transparent>
    </initialWindow>
    <author>ruedigergad.com</author>
    <copyright>Ruediger Gad, r.c.g@gmx.de, ruedigergad.com</copyright>
    <env value="app/native/imports:/usr/lib/qt4/imports" var="QML_IMPORT_PATH"/>
    <env value="app/native/plugins:app/native/lib:/usr/lib/qt4/plugins" var="QT_PLUGIN_PATH"/>
    <env value="app/native/lib:app/native/lib/qmf/lib:/usr/lib/qt4/lib/" var="LD_LIBRARY_PATH"/>
    <env value="app/native/lib/qmf/plugins" var="QMF_PLUGINS"/>
    <arg>-style</arg>
    <arg>qnxlight</arg>
    <action system="true">run_native</action>
    <!-- meepasswords should point to the project binary, path can be relative -->
    <asset path="meepasswords" type="Qnx/Elf" entry="true">meepasswords</asset>
    <asset path="%SRC_DIR%/logo_bb10.png">logo_bb10.png</asset>
    <asset path="%SRC_DIR%/lib/build/bb10">lib</asset>
    <icon>
        <image>logo_bb10.png</image>
    </icon>
    <authorId>gYAAgM1hVEHyVWp9kEL-6WK4ckQ</authorId>
</qnx>
